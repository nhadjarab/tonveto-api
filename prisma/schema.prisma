// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Auth {
  email String @unique @id
  passwordHash String
}

model User {
  id String @unique @id
  firstName String
  lastName String
  email String @unique
  birth_date DateTime
  phone_number String
  fully_registered Boolean
}

